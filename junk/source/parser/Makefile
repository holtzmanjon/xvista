uchrunch.o
LOBJS=$(MLIB)(bcrunch.o) $(MLIB)(bicrunch.o) $(MLIB)(crunch.o)                 \
$(MLIB)(ecrunch.o) $(MLIB)(errprint.o) $(MLIB)(expcrunch.o) $(MLIB)(fcrunch.o) \
$(MLIB)(fileinput.o) $(MLIB)(findexpr.o) $(MLIB)(findsym.o) $(MLIB)(getdec.o)  \
$(MLIB)(grammar.o) $(MLIB)(isanum.o) $(MLIB)(isit.o) $(MLIB)(lcrunch.o)        \
